rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /{allPaths=**} {
      allow read, create, update: if request.auth != null;
      allow delete: if request.auth != null && resource.contentType != 'text/plain';
    }

    match /public/{allPaths=**} {
      allow read;
    }
  }
}

